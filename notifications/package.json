{
  "name": "mollie-commercetools-notification-module",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "start": "node build/src/server.js",
    "develop": "nodemon src/server.ts",
    "develop:default-config": "CT_MOLLIE_CONFIG='{\"mollieApiKey\": \"default-api-key\"}' npm run develop",
    "zip-gcp-function": "npm ci && npm run build && cp build/gcpFunctionHandler.js build/index.js && cp -R node_modules build/ && cd build && rm gcpFunctionHandler.js lambdaHandler.js && zip -r ../notification-module.zip .",
    "sam-prepare": "npm ci && npm run build && cp build/lambdaHandler.js build/index.js && cp -R node_modules build/ && cp -r build ../deployment-examples/aws/notification/ && rm -rf build/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:component": "jest --testPathPattern=component",
    "lint": "prettier --check ./src ./tests ./config ./**/*.md",
    "lint:fix": "prettier --write ./src ./tests ./config ./**/*.md"
  },
  "keywords": [],
  "author": "Mollie",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.1",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.1",
    "nock": "^13.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@azure/functions": "^1.2.3",
    "@commercetools/platform-sdk": "^1.19.0",
    "@commercetools/sdk-client": "^2.1.2",
    "@commercetools/sdk-middleware-auth": "^6.1.4",
    "@commercetools/sdk-middleware-http": "^6.0.11",
    "@commercetools/sdk-middleware-logger": "^2.1.1",
    "@commercetools/sdk-middleware-user-agent": "^2.1.5",
    "@mollie/api-client": "^3.5.1",
    "express": "^4.17.1",
    "node-fetch-commonjs": "^3.0.3",
    "winston": "^3.3.3",
    "qs":"6.10.2"
  }
}
