{
  "name": "mollie-commercetools-notification-module",
  "version": "0.0.1",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "start": "node build/src/server.js",
    "develop": "nodemon src/server.ts",
    "develop:default-config": "CT_MOLLIE_CONFIG='{\"mollieApiKey\": \"default-api-key\"}' npm run develop",
    "zip-gcp-function": "npm ci && npm run build && cp build/gcpFunctionHandler.js build/index.js && cp -R node_modules build/ && cd build && zip -r ../extension-module.zip . && rm index.js",
    "zip-lambda": "npm ci && npm run build && cp build/lambdaHandler.js build/index.js && cp -R node_modules build/ && cd build && zip -r ../extension-module.zip . && rm index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "AND Digital",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.1",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@azure/functions": "^1.2.3",
    "@mollie/api-client": "^3.5.1",
    "express": "^4.17.1"
  }
}