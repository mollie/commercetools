{
  "name": "mollie-commercetools-notification-module",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "start": "node build/src/server.js",
    "develop": "nodemon src/server.ts",
    "develop:default-config": "CT_MOLLIE_CONFIG='{\"mollieApiKey\": \"default-api-key\"}' npm run develop",
    "zip-gcp-function": "npm ci && npm run build && cp build/gcpFunctionHandler.js build/index.js && cp -R node_modules build/ && cd build && rm gcpFunctionHandler.js lambdaHandler.js && zip -r ../notification-module.zip .",
    "zip-lambda": "npm ci && npm run build && cp build/lambdaHandler.js build/index.js && cp -R node_modules build/ && cd build && zip -r ../notification-module.zip . && rm index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters='text-summary'",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:component": "jest --testPathPattern=component",
    "lint": "prettier --check ./src ./tests ./config ./**/*.md",
    "lint:fix": "prettier --write ./src ./tests ./config ./**/*.md"
  },
  "keywords": [],
  "author": "Mollie",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "8.10.89",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "14.18.5",
    "@types/supertest": "2.0.11",
    "jest": "27.4.7",
    "nock": "13.2.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.2.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@azure/functions": "3.0.0",
    "@commercetools/platform-sdk": "2.1.0",
    "@commercetools/sdk-client": "2.1.2",
    "@commercetools/sdk-middleware-auth": "6.2.0",
    "@commercetools/sdk-middleware-http": "6.1.0",
    "@commercetools/sdk-middleware-logger": "2.1.1",
    "@commercetools/sdk-middleware-user-agent": "2.1.5",
    "@mollie/api-client": "3.5.1",
    "express": "4.17.2",
    "node-fetch-commonjs": "3.1.1",
    "winston": "3.4.0"
  }
}
